{
  "_source": {
    "exclude": [
      "chunked_text",
      "embeddings"
    ]
  },
  "query": {
    "bool": {
      "filter": [
        {
          "range": {
            "created_at": {
              "gte": 1756498133,
              "lte": 1756498133
            }
          }
        },
        {
          "range": {
            "file_size": {
              "gte": 0,
              "lte": 4096
            }
          }
        }
      ],
      "must": [
        {
          "match": {
            "file_path": "./test-document.docx"
          }
        },
        {
          "match": {
            "doc_part_id": 1
          }
        }
      ]
    }
  },
  "sort": [
    {
      "created_at": {
        "order": "desc"
      }
    }
  ]
}
